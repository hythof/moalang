main =
  src <- io.read
  llvm <- compile(src)
  io.write(llvm)

compile src = `@.str = private unnamed_addr constant [3 x i8] c"hi\00", align 1

define i32 @main() local_unnamed_addr #0 {
  %1 = tail call i64 @"\01_write"(i32 1, i8* getelementptr inbounds ([3 x i8], [3 x i8]* @.str, i64 0, i64 0), i64 2) #2
  ret i32 0
}

declare i64 @"\01_write"(i32, i8*, i64) local_unnamed_addr #1`
